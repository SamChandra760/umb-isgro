@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem<ImageBannerCarousel>>;

@* Render the block areas *@
<div class="owl-carousel events-carousel carousel-widget block-grid-imagebanner-carousel" data-margin="0"
     data-stage-padding="0" data-lazyload="true" data-loop="true" data-pagi="true"
      data-items="1" data-items-lg="1" data-items-xl="1">

    @foreach(IPublishedContent img in Model.Content.Images ?? [])
    {
        <div class="oc-item">
           <img src="@img.Url()" alt="@img.Name" />
        </div>
    }
</div>
