                                    </g>
                                    <defs>
                                        <clipPath id="clip0_128_603">
                                            <rect width="81" height="144" fill="white" />
                                        </clipPath>
                                    </defs>
                                </svg>
                            </div>
                            <div class="talk-content d-flex flex-column flex-grow-1" data-aos="fade-left">
                                <p>Telefon</p>
                                <a href="tel:@site.Value("telephone")">@site.Value("telephone")</a>
                            </div>
                        </div>
                    }
                    @if (site.HasValue("email"))
                    {
                        <div class="talk-item d-flex align-items-end">
                            <div class="talk-icon flex-grow-1" data-aos="fade-right">
