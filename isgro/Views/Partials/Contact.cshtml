@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage;
@{
    var site = Model.Root();
}
@if (Model.HasValue("cs_heading") || Model.HasValue("cs_description") || site.HasValue("telephone") || site.HasValue("email"))
{
    <section class="talk_section">
        <div class="container">
            <div class="section-heading">
                <div class="svg-icon" data-aos="fade-right">
                    <svg  viewBox="0 0 170 170" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <g clip-path="url(#clip0_188_82)">
                            <path d="M85 0C38.0545 0 0 38.0545 0 85C0 131.946 38.0545 170 85 170C131.946 170 170 131.946 170 85C170 38.0545 131.946 0 85 0ZM85 136C64.4385 136 47.294 128.699 43.35 119H126.65C122.715 128.699 105.562 136 85 136Z" fill="currentColor" />
                        </g>
                        <defs>
                            <clipPath id="clip0_188_82">
                                <rect width="170" height="170" fill="white" />
                            </clipPath>
                        </defs>
                    </svg>
                </div>
                <div class="heading" data-aos="fade-up">
                    @if (Model.HasValue("cs_heading"))
                    {
                        <h2>
                            @Html.Raw(Model.Value("cs_heading"))
                        </h2>                    
                    }
                </div>
            </div>
            @if (Model.HasValue("cs_description"))
            {
                <div class="d-flex flex-row justify-content-end">
                    <div class="description" data-aos="fade-up">
                         @Html.Raw(Model.Value("cs_description"))
                    </div>
                </div>
            }
            @if (site.HasValue("telephone") || site.HasValue("email"))
            {
                <div class="s-h-p-l d-flex flex-column">
                    @if (site.HasValue("telephone"))
                    {
                        <div class="talk-item d-flex align-items-end">
                            <div class="talk-icon flex-grow-1" data-aos="fade-right">
