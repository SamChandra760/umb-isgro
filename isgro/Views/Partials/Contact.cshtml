                                    </g>
                                    <defs>
                                        <clipPath id="clip0_128_609">
                                            <rect width="129.36" height="136.78" fill="white" />
                                        </clipPath>
                                    </defs>
                                </svg>
                            </div>
                            <div class="talk-content d-flex flex-column flex-grow-1" data-aos="fade-left">
                                <p>E.Mail</p>
                                <a href="mailto:@site.Value("email")">@site.Value("email")</a>
                            </div>
                        </div>
                    }
                </div>
            }
        </div>
    </section>
}